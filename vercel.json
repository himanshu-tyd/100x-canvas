{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "dist/**",
            "../../packages/backend-common/dist/**",
            "../../packages/db/dist/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ]
  }
  