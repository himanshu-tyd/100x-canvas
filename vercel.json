{
    "version": 2,
    "builds": [
        {
            "src": "apps/http-backend/dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "apps/http-backend/dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "apps/http-backend/dist/index.js"
        }
    ]
}
